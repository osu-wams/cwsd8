    "conflict": {
        "drupal/drupal": "*"
    },
    "autoload": {
        "classmap": [
            "scripts/composer/SiteGenerator.php"
        ]
    },
    "scripts": {
        "generate-site": "osuwams\\SiteGenerator::generate",
        "post-drupal-scaffold-cmd": [
            "cd docroot && patch -p1 <../patches/htaccess-updats.patch"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "platform": {
            "php": "8.1.12"
        },
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "mnsami/composer-custom-directory-installer": true,
            "oomphinc/composer-installers-extender": true
        }
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/core": {
                "3062376-44 Template suggestions for custom block view modes": "https://git.drupalcode.org/project/drupal/-/merge_requests/2646.patch",
                "2951547-50 Modify LB Off-canvas width": "https://www.drupal.org/files/issues/2022-07-13/f245f21ea664f22c81d8d28c6f0f4a42fb9a5890.patch",
                "3049332-60 Layout Builder getEntityTypeId null": "https://www.drupal.org/files/issues/2021-11-23/3049332-60.patch",
                "3162699-13 Layout Builder Debuging patch": "https://www.drupal.org/files/issues/2020-08-10/3162699-blockbuild-13.patch",
                "2719797-144 Use admin theme option on views": "https://www.drupal.org/files/issues/2022-09-20/2719797-144.patch",
                "2605218-94 Views Block Display skips preBlockBuild() call on ajax rebuild": "https://www.drupal.org/files/issues/2023-03-03/2605218-94.patch",
                "3123350-29 Private File Migration path fix": "https://www.drupal.org/files/issues/2022-10-10/patch-29.patch"
            },
            "drupal/bootstrap_layout_builder": {
                "3203096 Column Alignment": "https://git.drupalcode.org/project/bootstrap_layout_builder/-/merge_requests/19.patch"
            },
            "drupal/bootstrap_styles": {
                "3252403-7 Animations not applying data attribute": "https://www.drupal.org/files/issues/2022-12-01/animation-effect-3252403-7.patch",
                "3315218-2 AOS Library loading": "https://www.drupal.org/files/issues/2022-11-26/aos.local_not_loading_3315218_2.patch"
            },
            "drupal/ckeditor": {
                "2876094-39 Adding extra p to ckeditor links": "https://www.drupal.org/files/issues/2023-01-31/2876094-39.patch",
                "3330723-3 Media alignment": "https://www.drupal.org/files/issues/2023-01-03/3330723-3.patch"
            },
            "drupal/ctools": {
                "2759445-100 Add option to pass contextual filters to views blocks": "https://www.drupal.org/files/issues/2023-01-18/2759445-100.patch"
            },
            "drupal/default_content": {
                "3160146-52 Add Normalizer/Denormalizer for LB support": "https://www.drupal.org/files/issues/2022-05-15/default_content-3160146-52.patch"
            },
            "drupal/entitygroupfield": {
                "3183134-6": "https://www.drupal.org/files/issues/2022-04-04/entitygroupfield-matching-expected-dropbutton-markup-3183134-6.patch"
            },
            "drupal/feeds_tamper": {
                "2983197-17 fix tamper error": "https://www.drupal.org/files/issues/2022-04-22/feeds_tamper-tamper-exception-catching-2983197-17.patch"
            },
            "drupal/group": {
                "2815971-32 more contexts": "https://www.drupal.org/files/issues/2022-04-21/group-context-2815971-32.patch",
                "3256998-35 revisions": "https://www.drupal.org/files/issues/2022-03-30/group-3256998-35.patch"
            },
            "drupal/group_content_menu": {
                "3211822-18 Update node later logic": "https://www.drupal.org/files/issues/2022-05-05/3211822-18.patch"
            },
            "drupal/google_cse": {
                "3269510 as_sitesearch not working": "https://git.drupalcode.org/project/google_cse/-/merge_requests/4.patch"
            },
            "drupal/paragraphs": {
                "3090200-22 Paragraphs Access Check in LB" : "https://www.drupal.org/files/issues/2023-01-31/3084934-13_combine_3090200-22-update_0.patch"
            },
            "drupal/simple_popup_blocks": {
                "3040744-14 Update show when delay is enabled": "https://www.drupal.org/files/issues/2022-10-28/delayed-popup-content-invisible-3040744-14.patch"
            },
            "drupal/toc_js": {
                "3334941-1 ToC Back to Top repeats": "https://www.drupal.org/files/issues/2023-01-19/big-pipe-toc-js-back-to-top-repeating.patch"
            },
            "drupal/token" :{
                "1198032-54": "https://www.drupal.org/files/issues/2021-08-11/1198032-token-paginated-54.patch"
            }
        },
        "drupal-scaffold": {
            "file-mapping": {
                "[web-root]/README.md": false,
                "[web-root]/example.gitignore": false
            },
            "locations": {
                "web-root": "docroot/"
            }
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/libraries/ckeditor/plugins/{$name}": [
                "drupal-ckeditor-libraries-group/div",
                "drupal-ckeditor-libraries-group/liststyle"
            ],
            "docroot/libraries/slick": [
                "npm-asset/slick-carousel"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:npm-asset",
                "type:bower-asset"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "docroot/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "docroot/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "docroot/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        }
    }
