        "osu-wams/osu_block_types": "^1.0",
        "osu-wams/osu_bootstrap_layout_builder": "^1.0",
        "osu-wams/osu_ckeditor_plugins": "^1.0",
        "osu-wams/osu_default_content": "^1.0",
        "osu-wams/osu_groups": "^1.0@alpha",
        "osu-wams/osu_media": "^1.0",
        "osu-wams/osu_profile": "^1.0",
        "osu-wams/osu_roles": "^1.0",
        "osu-wams/osu_seckit": "^2.0",
        "osu-wams/osu_standard": "@dev",
        "osu-wams/osu_story": "^1.0",
        "toc/toc": "*"
    },
    "require-dev": {
        "drupal/core-dev": "9.4.7",
        "drupal/devel": "^5.0",
        "kint-php/kint": "^4.1",
        "npm-asset/d3": "^3",
        "npm-asset/highlightjs": "^9.16",
        "phpspec/prophecy-phpunit": "^2"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "autoload": {
        "classmap": [
            "scripts/composer/SiteGenerator.php"
        ]
    },
    "scripts": {
        "generate-site": "osuwams\\SiteGenerator::generate",
        "post-drupal-scaffold-cmd": [
            "cd docroot && patch -p1 <../patches/htaccess-updats.patch"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "platform": {
            "php": "7.4.26"
        },
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "mnsami/composer-custom-directory-installer": true,
            "oomphinc/composer-installers-extender": true
        }
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/core": {
                "3044117-34 layout builder updates": "https://www.drupal.org/files/issues/2021-10-18/expose-properties-config-section-form-3044117-34.patch",
                "2985882-142 layout builder workaround": "https://www.drupal.org/files/issues/2022-05-30/2985882-field-142.patch",
                "3062376-34 Template suggestions for custom block view modes": "https://www.drupal.org/files/issues/2020-07-02/3062376-34.patch",
                "3102249-42 Media alignment CKEditor": "https://www.drupal.org/files/issues/2022-04-13/3102249-42.patch",
                "2951547-50": "https://www.drupal.org/files/issues/2022-07-13/f245f21ea664f22c81d8d28c6f0f4a42fb9a5890.patch",
                "2719797-137 Use admin theme option on views": "https://www.drupal.org/files/issues/2022-01-17/2719797-137-9.3.x.patch",
                "3131055-17 Adding extra p to ckeditor": "https://www.drupal.org/files/issues/2021-04-19/ckeditor-p-tag-added-to-media-3131055-17.patch",
                "3119786-42 Layout builder default values": "https://www.drupal.org/files/issues/2022-02-08/3119786-defaultvalue-42.patch",
                "2830725-23 Add block view modes to classes": "https://www.drupal.org/files/issues/2021-10-19/2830725-23.patch"
            },
            "drupal/bootstrap_layout_builder": {
                "3203096 Column Alignment": "https://git.drupalcode.org/project/bootstrap_layout_builder/-/merge_requests/19.patch"
            },
            "drupal/ctools": {
                "2759445-86 Add option to pass contextual filters to views blocks": "https://www.drupal.org/files/issues/2022-05-22/2759445-86.patch"
            },
            "drupal/default_content": {
                "3160146-52 Add Normalizer/Denormalizer for LB support": "https://www.drupal.org/files/issues/2022-05-15/default_content-3160146-52.patch"
            },
            "drupal/entitygroupfield": {
                "3183134-6": "https://www.drupal.org/files/issues/2022-04-04/entitygroupfield-matching-expected-dropbutton-markup-3183134-6.patch"
            },
            "drupal/group": {
                "2815971-32 more contexts": "https://www.drupal.org/files/issues/2022-04-21/group-context-2815971-32.patch"
            },
            "drupal/group_content_menu": {
                "3211822-18 Update node later logic": "https://www.drupal.org/files/issues/2022-05-05/3211822-18.patch"
            },
            "drupal/google_cse": {
                "3269510 as_sitesearch not working": "https://git.drupalcode.org/project/google_cse/-/merge_requests/4.patch"
            }
        },
        "drupal-scaffold": {
            "file-mapping": {
                "[web-root]/README.md": false,
                "[web-root]/example.gitignore": false
            },
            "locations": {
                "web-root": "docroot/"
            }
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/libraries/ckeditor/plugins/{$name}": [
                "drupal-ckeditor-libraries-group/div"
            ],
            "docroot/libraries/slick": [
                "npm-asset/slick-carousel"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:npm-asset",
                "type:bower-asset"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "docroot/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "docroot/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "docroot/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        }
    }
}
