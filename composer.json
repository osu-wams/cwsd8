        "drupal/asset_injector": "^2.6",
        "drupal/auto_entitylabel": "3.x-dev",
        "drupal/better_exposed_filters": "^6.0",
        "drupal/block_class": "^2.0",
        "drupal/block_classes": "^1.0",
        "drupal/block_list_override": "^1.0",
