        "drupal/libraries": "^3.0@alpha",
        "drupal/linkit": "^5.0@beta",
        "drupal/media_bulk_upload": "^3.0",
        "drupal/media_library_theme_reset": "^1.0@beta",
        "drupal/memcache": "^2.5",
        "drupal/menu_breadcrumb": "^1.16",
        "drupal/menu_item_extras": "^2.10",
        "drupal/metatag": "^1.19",
        "drupal/migrate_plus": "^6.0",
