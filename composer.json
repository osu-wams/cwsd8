{
    "name": "osu-wams/drupal",
    "description": "Drupal Distribution for Oregon State Web and Mobile Services",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "vcs",
            "url": "https://github.com/osu-wams/madrone.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/osu-wams/osu_groups.git"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal/acquia_connector": "^3.0",
        "drupal/acquia_purge": "^1.0",
        "drupal/admin_toolbar": "^3.0",
        "drupal/asset_injector": "^2.6",
        "drupal/block_list_override": "^1.0",
        "drupal/cas": "^1.5",
        "drupal/cas_attributes": "^1.0@beta",
        "drupal/config_ignore": "^2.2",
        "drupal/config_split": "^1.4",
        "drupal/core-composer-scaffold": "^8.8",
        "drupal/core-recommended": "^8.8",
        "drupal/entity_clone": "^1.0@beta",
        "drupal/entitygroupfield": "^1.0@alpha",
        "drupal/fontawesome_menu_icons": "^1.7",
        "drupal/google_cse": "^3.2",
        "drupal/group": "^1.3",
        "drupal/group_content_menu": "^1.0@RC",
        "drupal/layout_builder_modal": "^1.0@alpha",
        "drupal/layout_builder_restrictions": "^2.3",
        "drupal/layout_builder_styles": "^1.0@beta",
        "drupal/lb_ux": "^1.0@alpha",
        "drupal/libraries": "^3.0@alpha",
        "drupal/media_library_theme_reset": "^1.0@beta",
        "drupal/memcache": "^2.0",
        "drupal/menu_item_extras": "^2.10",
        "drupal/module_filter": "^3.1",
        "drupal/pathauto": "^1.6",
        "drupal/redirect": "^1.6",
        "drupal/smart_trim": "^1.2",
        "drupal/tour_ui": "@beta",
        "drupal/views_layout": "^1.1",
        "drush/drush": "^10",
        "mnsami/composer-custom-directory-installer": "^2.0",
        "npm-asset/fontawesome-iconpicker": "^3.2",
        "npm-asset/fonticonpicker--fonticonpicker": "^3.1",
        "oomphinc/composer-installers-extender": "^2.0",
        "osu-wams/madrone": "1.x-dev",
        "osu-wams/osu_groups": "^1.0@alpha"
    },
    "require-dev": {
        "bower-asset/highlightjs": "^9.7",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
        "drupal/core-dev": "^8.8",
        "drupal/devel": "^4",
        "kint-php/kint": "^3.3",
        "npm-asset/d3": "^3"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "autoload": {
        "classmap": [
            "scripts/composer/SiteGenerator.php"
        ]
    },
    "scripts": {
        "generate-site": "osuwams\\SiteGenerator::generate"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/core": {
                "3044117 layout builder updates": "https://www.drupal.org/files/issues/2019-10-31/expose-properties-config-section-form-with-tests-3044117-13.patch",
                "3040354 entity type annotation": "https://www.drupal.org/files/issues/2019-10-28/3040354-10--will-fail.patch",
                "3062376 Template suggestions for custom block view modes": "https://www.drupal.org/files/issues/2019-12-30/3062376-15.patch"
            },
            "drupal/group": {
                "2815971 more contexts": "https://www.drupal.org/files/issues/2021-01-25/group_context_on_group_content-2815971-23.patch"
            },
            "drupal/layout_builder_styles": {
                "3111518 LB Styles Updates": "https://www.drupal.org/files/issues/2020-05-20/layout_styles_get_current_layout-3111518-6.patch"
            }
        },
        "drupal-scaffold": {
            "file-mapping": {
                "[web-root]/README.txt": false,
                "[web-root]/example.gitignore": false
            },
            "locations": {
                "web-root": "docroot/"
            }
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:npm-asset",
                "type:bower-asset"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "docroot/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "docroot/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "docroot/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        }
    }
}
