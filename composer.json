{
    "name": "osu-wams/drupal",
    "description": "Drupal Distribution for Oregon State Web and Mobile Services",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "vcs",
            "url": "https://github.com/osu-wams/madrone.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/osu-wams/osu_groups.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/osu-wams/osu_standard.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/osu-wams/osu_media.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/osu-wams/osu_block_types.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/osu-wams/osu_ckeditor_plugins.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/osu-wams/osu_seckit.git"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal-ckeditor-libraries-group/codesnippet": "^4.17",
        "drupal-ckeditor-libraries-group/div": "^4.17",
        "drupal/acquia_connector": "^3.0",
        "drupal/acquia_purge": "^1.0",
        "drupal/address": "^1.9",
        "drupal/admin_toolbar": "^3.0",
        "drupal/asset_injector": "^2.6",
        "drupal/better_exposed_filters": "^5.0",
        "drupal/block_classes": "^1.0",
        "drupal/block_list_override": "^1.0",
        "drupal/block_visibility_groups": "^1.4",
        "drupal/bootstrap_layout_builder": "^2.0",
        "drupal/cas": "^2.0@beta",
        "drupal/cas_attributes": "^2@beta",
        "drupal/ckeditor_config": "^3.1",
        "drupal/ckeditor_div_manager": "^2.0",
        "drupal/ckeditor_scayt": "^1.3",
        "drupal/codesnippet": "^1.7",
        "drupal/component": "@rc",
        "drupal/config_ignore": "^2.2",
        "drupal/config_split": "^1.4",
        "drupal/core-composer-scaffold": "^9.2",
        "drupal/core-recommended": "^9.2",
        "drupal/diff": "^1.0",
        "drupal/editor_advanced_link": "^2.0",
        "drupal/editor_file": "^1.6",
        "drupal/entity_clone": "^1.0@beta",
        "drupal/entitygroupfield": "^1.0@alpha",
        "drupal/exclude_node_title": "^1.3",
        "drupal/field_group": "^3.1",
        "drupal/fontawesome_menu_icons": "^1.7",
        "drupal/google_cse": "^3.2",
        "drupal/google_tag": "^1.4",
        "drupal/group": "^1.3",
        "drupal/group_content_menu": "^1.0@RC",
        "drupal/layout_builder_blocks": "^1.0",
        "drupal/layout_builder_component_attributes": "^2.0",
        "drupal/layout_builder_modal": "^1.0@alpha",
        "drupal/layout_builder_operation_link": "^1.0",
        "drupal/layout_builder_restrictions": "^2.3",
        "drupal/libraries": "^3.0@alpha",
        "drupal/linkit": "^5.0@beta",
        "drupal/media_bulk_upload": "^3.0",
        "drupal/media_library_theme_reset": "^1.0@beta",
        "drupal/memcache": "^2.0",
        "drupal/menu_breadcrumb": "^1.16",
        "drupal/menu_item_extras": "^2.10",
        "drupal/metatag": "^1.19",
        "drupal/migrate_plus": "^5.1",
        "drupal/module_filter": "^3.1",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": "^1.6",
        "drupal/redirect": "^1.6",
        "drupal/responsive_preview": "^1.0",
        "drupal/seckit": "^2.0",
        "drupal/slick": "^2.6",
        "drupal/slick_views": "^2.6",
        "drupal/smart_trim": "^1.2",
        "drupal/superfish": "^1.4",
        "drupal/tb_megamenu": "^1.6",
        "drupal/tour_ui": "@beta",
        "drupal/views_bootstrap": "^4.3",
        "drupal/views_bulk_edit": "^2.6",
        "drupal/views_bulk_operations": "^4.1",
        "drupal/views_conditional": "^1.4",
        "drupal/views_data_export": "^1.1",
        "drupal/views_layout": "^1.1",
        "drupal/webform": "^6.1",
        "drush/drush": "^11.0",
        "mnsami/composer-custom-directory-installer": "^2.0",
        "npm-asset/blazy": "^1.8",
        "npm-asset/dropzone": "^5.9",
        "npm-asset/fontawesome-iconpicker": "^3.2",
        "npm-asset/fonticonpicker--fonticonpicker": "^3.1",
        "npm-asset/slick-carousel": "1.8.0",
        "oomphinc/composer-installers-extender": "^2.0",
        "osu-wams/madrone": "^1.0.0@alpha",
        "osu-wams/osu_block_types": "@dev",
        "osu-wams/osu_ckeditor_plugins": "@dev",
        "osu-wams/osu_groups": "^1.0@alpha",
        "osu-wams/osu_media": "@dev",
        "osu-wams/osu_seckit": "^2.0",
        "osu-wams/osu_standard": "@dev"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
        "drupal/core-dev": "^9.2",
        "drupal/devel": "^4",
        "kint-php/kint": "^3.3",
        "npm-asset/d3": "^3",
        "npm-asset/highlightjs": "^9.16",
        "phpspec/prophecy-phpunit": "^2"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "autoload": {
        "classmap": [
            "scripts/composer/SiteGenerator.php"
        ]
    },
    "scripts": {
        "generate-site": "osuwams\\SiteGenerator::generate",
        "post-drupal-scaffold-cmd": [
            "cd docroot && patch -p1 <../patches/htaccess-updats.patch"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "platform": {
            "php": "7.4.26"
        },
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "mnsami/composer-custom-directory-installer": true,
            "oomphinc/composer-installers-extender": true
        }
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/core": {
                "3044117-34 layout builder updates": "https://www.drupal.org/files/issues/2021-10-18/expose-properties-config-section-form-3044117-34.patch",
                "3062376-34 Template suggestions for custom block view modes": "https://www.drupal.org/files/issues/2020-07-02/3062376-34.patch",
                "3102249-21": "https://www.drupal.org/files/issues/2020-06-01/3102249-21.patch"
            },
            "drupal/entitygroupfield": {
                "3183134-6": "https://www.drupal.org/files/issues/2022-04-04/entitygroupfield-matching-expected-dropbutton-markup-3183134-6.patch"
            },
            "drupal/group": {
                "2815971 more contexts": "https://www.drupal.org/files/issues/2021-01-25/group_context_on_group_content-2815971-23.patch"
            }
        },
        "drupal-scaffold": {
            "file-mapping": {
                "[web-root]/README.txt": false,
                "[web-root]/example.gitignore": false
            },
            "locations": {
                "web-root": "docroot/"
            }
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/libraries/ckeditor/plugins/{$name}": [
                "drupal-ckeditor-libraries-group/div"
            ],
            "docroot/libraries/slick": ["npm-asset/slick-carousel"],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:npm-asset",
                "type:bower-asset"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "docroot/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "docroot/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "docroot/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        }
    }
}
