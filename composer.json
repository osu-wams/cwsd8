        "drupal/editoria11y": "^1.0",
        "drupal/entity_clone": "^1.0@beta",
        "drupal/entitygroupfield": "^1.0@alpha",
        "drupal/exclude_node_title": "^1.3",
        "drupal/feeds": "^3.0beta2",
        "drupal/feeds_ex": "^1.0beta",
        "drupal/feeds_tamper": "^2.0beta2",
        "drupal/field_group": "^3.1",
        "drupal/fontawesome_menu_icons": "^1.7",
        "drupal/google_cse": "^3.2",
        "drupal/google_tag": "^1.4",
        "drupal/group": "^1.3",
        "drupal/group_content_menu": "^1.0@RC",
        "drupal/image_effects": "^3.2",
        "drupal/jsonapi_extras": "^3.21",
        "drupal/layout_builder_blocks": "^1.0",
